{
  "functions": {
    "app/api/admin/**": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0, s-maxage=0"
        },
        {
          "key": "X-Vercel-Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
